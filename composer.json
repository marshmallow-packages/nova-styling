{
    "name": "marshmallow/nova-styling",
    "description": "A Responsive Laravel Nova Marshmallow Theme based on 'A Responsive Laravel Nova theme'. With dark mode :)",
    "keywords": [
        "Laravel Nova Theme",
        "Marshmallow Theme",
        "Dark Mode",
        "Laravel Nova Dark Mode",
        "Marshmallow",
        "laravel",
        "nova",
        "Responsive Laravel Nova Theme"
    ],
    "homepage": "https://github.com/marshmallow-packages/nova-styling",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev",
            "homepage": "https://marshmallow.dev/",
            "role": "Developer"
        },
        {
            "name": "LTKort",
            "email": "lars@kort.dev",
            "homepage": "https://kort.dev/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "marshmallow/categorise-resources": "^1.0.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Marshmallow\\NovaStyling\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marshmallow\\NovaStyling\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\NovaStyling\\ThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
